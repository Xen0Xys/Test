{
    "name": "fullstack-template",
    "private": true,
    "workspaces": [
        "client",
        "server"
    ],
    "scripts": {
        "start:client": "pnpm --prefix client run dev",
        "start:server": "pnpm --prefix server run start",
        "start:all": "concurrently \"pnpm run start:client\" \"pnpm run start:server\""
    },
    "devDependencies": {
        "concurrently": "^9.0.1"
    }
}
