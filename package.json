{
    "name": "fullstack-template",
    "private": true,
    "workspaces": [
        "client",
        "server"
    ],
    "scripts": {
        "dev:client": "pnpm --prefix client run dev",
        "dev:server": "pnpm --prefix server run dev",
        "dev:all": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\""
    },
    "devDependencies": {
        "concurrently": "^9.0.1"
    }
}
